generator client {
  provider = "prisma-client-js"
}

datasource db {
  provider = "sqlite"
  url      = "file:./data.db"
}

model User {
  id       String   @id @default(cuid())
  username String   @unique
  session  Session?
}

model Session {
  id          String   @id @default(cuid())
  user        User     @relation(fields: [userId], references: [id])
  userId      String   @unique
  accessToken String
  expiresAt   DateTime
}
